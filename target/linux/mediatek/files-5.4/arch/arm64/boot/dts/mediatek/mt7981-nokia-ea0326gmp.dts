/dts-v1/;
#include "mt7981-nokia-ea0326gmp.dtsi"

/ {
nmbm_spim_nand {
		compatible = "generic,nmbm";

		#address-cells = <1>;
		#size-cells = <1>;

		lower-mtd-device = <&spi_nand>;
		forced-create;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "BL2";
				reg = <0x00 0x100000>;
			};

			partition@100000 {
				label = "u-boot-env";
				reg = <0x100000 0x80000>;
			};

			partition@180000 {
				label = "Factory";
				reg = <0x180000 0x200000>;
			};

			partition@380000 {
				label = "FIP";
				reg = <0x380000 0x200000>;
			};

			partition@580000 {
				label = "Config";
				reg = <0x580000 0x200000>;
			};

			partition@780000 {
				label = "Config2";
				reg = <0x780000 0x200000>;
			};

			partition@980000 {
				label = "Aos-net";
				reg = <0x980000 0xc00000>;
			};

			partition@1580000 {
				label = "bvasPlugin";
				reg = <0x1580000 0xc00000>;
			};

			partition@2180000 {
				label = "ubi";
				reg = <0x2180000 0x5680000>;
			};
		};
	};
}